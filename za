package zakir;
class Solution {
    public void findMaxK() {
        int[] nums = {-1, 2, -3, 3};
        int n = 0;
        for (int i = 0; i < nums.length; i++) {
            if (nums[i] >n) {
                n = nums[i];
            }
        }
        System.out.println(n);
    }
}

    public class leetcode {
        public static void main(String[] args) {
            Solution sol = new Solution();
            sol.findMaxK();
        }
    }

