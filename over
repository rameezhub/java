package zakir;


class animal1 {
    String dog;
    int x;
    animal1(){
     System.out.println("this is the first constructor");
 }

     void animal2(){
        System.out.println("this is the first constructor");
    }

    public String getdog() {
        return dog;
    }
    public void setdog(String dog) {
        System.out.println("set the dog name");
        this.dog = dog;
    }
}
class  Derived  extends animal1 {
    @Override
    void  animal2(){
        System.out.println("this is animal of  derived class");
     }
    Derived(){
        System.out.println("this is class 2");
    }
    String cat;

    public  String getcat(){
        return cat;}
    public void setcat(String cat){
        System.out.println("set the cat  name");
        this.cat=cat;
    }
}
